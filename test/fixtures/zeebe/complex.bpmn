    <bpmn:serviceTask id="Activity_0de20jk" name="Receive Forum Posts for Query" zeebe:modelerTemplate="io.camunda.connectors.HttpJson.v2" zeebe:modelerTemplateVersion="5" zeebe:modelerTemplateIcon="data:image/svg+xml;base64,">
